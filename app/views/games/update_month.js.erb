<% if @games.empty? %>
  $("<%= "month"+@m %>").update("No Games with these filters.");
<% else %>
  <% if @limit > 0 %>
    $("<%= "month"+@m %>").update("<%= escape_javascript(render 'month_view') %>");
  <% else %>
    $("<%= "month"+@m %>").update("<%= escape_javascript(render 'month_view_all') %>");
  <% end %>
<% end %>
